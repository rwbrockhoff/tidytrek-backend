{
  "name": "tt-backend",
  "version": "1.0.0",
  "description": "",
  "author": "Ryan Brockhoff <brockhoffrw@gmail.com>",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node dist/server/index.js",
    "dev": "NODE_ENV=development tsx watch server/index.ts",
    "test": "NODE_ENV=test vitest",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "migrate:make": "NODE_OPTIONS='--import=tsx' knex migrate:make",
    "migrate:latest": "NODE_OPTIONS='--import=tsx' knex migrate:latest",
    "migrate:rollback": "NODE_OPTIONS='--import=tsx' knex migrate:rollback",
    "migrate:unlock": "NODE_OPTIONS='--import=tsx' knex migrate:unlock",
    "seed:make": "NODE_OPTIONS='--import=tsx' knex seed:make",
    "seed:run" : "NODE_OPTIONS='--import=tsx' knex seed:run"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@vitest/coverage-v8": "^1.1.0",
    "supertest": "^6.3.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "camelcase": "^8.0.0",
    "camelcase-keys": "^9.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "rimraf": "^5.0.5",
    "snakecase-keys": "^5.5.0"
  }
}
